{
    "manifest_version": 3,
    "name": "Bring the Twitter Bird Back",
    "version": "0.1",
    "description": "A simple Chrome extension to bring the Twitter bird everyone loves back to the Twitter website.",
    "host_permissions": [
        "*://*.twitter.com/*"
    ],
    "web_accessible_resources": [{
        "resources": ["/images/*"], 
        "matches": ["*://*.twitter.com/*"]
    }],
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage",
        "scripting",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    }
}
  